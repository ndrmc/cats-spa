<div class="right_col" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Operations page</h3>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>New Operation</h2>
                        <ul class="nav navbar-right panel_toolbox">
                            <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                <ul class="dropdown-menu" role="menu">
                                    <li><a href="#">Settings 1</a>
                                    </li>
                                    <li><a href="#">Settings 2</a>
                                    </li>
                                </ul>
                            </li>
                            <li><a class="close-link"><i class="fa fa-close"></i></a>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">                       
                    <br>
                    <form id="new-operation-form" data-parsley-validate="" class="form-horizontal form-label-left" novalidate="" method="POST">

                      <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Name <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <input type="text" name="name" required="required" class="form-control col-md-7 col-xs-12">
                        </div>
                      </div>
                      <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Description <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <input type="text" name="description" required="required" class="form-control col-md-7 col-xs-12">
                        </div>
                      </div>
                      <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Year <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <input type="text" name="year" required="required" class="form-control col-md-7 col-xs-12">
                        </div>
                      </div>
                       <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Round <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <input type="text" name="round" required="required" class="form-control col-md-7 col-xs-12">
                        </div>
                      </div>
                      <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Program <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <select  class="form-control col-md-7 col-xs-12" name="programId" required="">
                            <option value="">Choose..</option>
                            {{#each programs}}
                            <option value={{this.ProgramID}}>{{this.Name}}</option>
                             {{/each}}
                          </select>
                        </div>
                      </div>
                      <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">HRD / PSNP Plan</label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                           <select  class="form-control col-md-7 col-xs-12" name="planId" required="">
                            <option value="">Choose..</option>
                            {{#each plans}}
                            <option value={{this.PlanID}}>{{this.PlanName}}</option>
                             {{/each}}
                          </select>
                        </div>
                      </div>
                      <div class="form-group">
                          
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Start</label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          {{!--<input class="form-control col-md-7 col-xs-12" type="text" name="expectedStart">--}}
                         <input  class="date-picker form-control col-md-7 col-xs-12" required="required" type="date" name="expectedStart" >
                          {{!--<input type="hidden" id="my_hidden_input"> --}}   

                        </div>
                      </div>

                     <div class="form-group">                          
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">End</label>
                         <div class="col-md-6 col-sm-6 col-xs-12">
                           <input  class="date-picker form-control col-md-7 col-xs-12" required="required" type="date" name="plannedEnd">
                          {{!--<input type="hidden" id="my_hidden_input"> --}}   

                        </div>                      
                      </div>
                      
                     <div class="form-group">                          
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Ration</label>
                         <div class="col-md-6 col-sm-6 col-xs-12">
                          <select  class="form-control col-md-7 col-xs-12" name="rationId" required="">
                            <option value="">Choose..</option>
                            {{#each rations}}
                            <option value={{this.RationID}}>{{this.RefrenceNumber}}</option>
                             {{/each}}
                          </select>
                        </div>                      
                      </div>
                       <div class="form-group">                          
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Status</label>
                         <div class="col-md-6 col-sm-6 col-xs-12">
                          <input class="form-control col-md-7 col-xs-12" type="text" name="status">
                        </div>                      
                      </div>

                    <div class="form-group">
                        <label class="col-md-3 col-sm-3 col-xs-12 control-label">Regions                         
                        </label>
                        
                        
                        <div class="col-md-9 col-sm-9 col-xs-12">
                           {{#each regions}}
                          <div class="checkbox">
                            <label>
                              <input type="checkbox"name="region" value={{this.AdminUnitID}}> {{this.Name}}
                            </label>
                          </div>
                           {{/each}}                         
                        </div>
                      </div>
                      <div class="form-group">                          
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Remark</label>
                         <div class="col-md-6 col-sm-6 col-xs-12">
                         <textarea class="resizable_textarea form-control" style="overflow: hidden; word-wrap: break-word; resize: horizontal; height: 53.6px;" name="remark"></textarea>
                        </div>                      
                      </div>

                      <div class="ln_solid"></div>
                      <div class="form-group">
                        <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                          <button type="submit" class="btn btn-primary">Close</button>
                          <button type="submit" class="btn btn-success">Save</button>
                        </div>
                      </div>

                    </form>

                    </div>
                </div>
            </div>
        </div>
    </div>

<script type = "text/javascript" >
    $("input:checkbox[name=region]:checked").each(function () {
        operationRegions.push($(this).val());
    });    

</script>
 